testcase tc_jsonBasic_encdec_os_union() runs on CT {

  //input union possibilities:
  var RoU rou :={ 
    { i := 1 },
    { bs := '1'B },
    { cs := "1"},
    { hs :='1'H },
    { os :='01'O },
    { ucs := char(0,0,0,1) },
    {  e:= first }
  }
  var U_json z; //decoded value
  //expected strings:
  var RoOS es := {
    unichar2oct("{\"i\":1}"),
    unichar2oct("{\"bs\":\"1\"}"),
    unichar2oct("{\"cs\":\"1\"}"),
    unichar2oct("{\"hs\":\"1\"}"),
    unichar2oct("{\"os\":\"01\"}"),
    unichar2oct("{\"ucs\":\"\\u0001\"}"),
    unichar2oct("{\"e\":\"first\"}")
  }
  var octetstring os;
  var integer N:= sizeof(rou);
  for( var integer i:=0; i<N;i:=i+1) {
    log("expected:", es[i]);
    os := f_json_enc_u2os(rou[i]);
    log(os);
    log(oct2char(os));
    f_compare_octetstring(os,es[i]);

    // z = encvalue(rou[i]); // it would be a octetstring value
    //decode test:
    z := f_json_dec_os2u(es[i]);
    if ( rou[i]==z ) {
      setverdict(pass);
    } else {
      setverdict(fail);
    }
  }
}

